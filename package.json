{
  "name": "@flanksource/flanksource-ui",
  "version": "0.2.1",
  "private": false,
  "files": [
    "build",
    "README.md",
    "scripts/serve-build.js"
  ],
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "@hookform/resolvers": "^2.8.8",
    "@storybook/client-api": "^6.5.7",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "ansi-to-html": "^0.7.2",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.4",
    "history": "^5.0.1",
    "http-proxy-middleware": "^2.0.2",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^1.3.3",
    "prop-types": "^15.8.1",
    "qs": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-image": "^4.0.3",
    "react-loading-icons": "^1.0.8",
    "react-mentions": "4.4.0",
    "react-query": "^3.34.15",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.2.2",
    "react-table": "^7.7.0",
    "react-tooltip": "^4.2.21",
    "recharts": "^2.1.10",
    "tailwindcss": "^3.0.24",
    "timeago.js": "^4.0.2",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "proxy": "https://incident-commander.canary.lab.flanksource.com",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development npx eslint --fix --ext .js,.jsx,.ts,.tsx"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "cross-env NODE_ENV=development npx prettier --write"
    ]
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true NODE_ENV=production react-scripts build",
    "serve-build": "gzip -f --keep build/static/*/*.{js,css} && node scripts/serve-build.js",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(history))/\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "lint:eslint": "cross-env NODE_ENV=development eslint --ext .js,.jsx,.ts,.tsx .",
    "fix:eslint": "cross-env NODE_ENV=development eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check:browserslist": "npx browserslist",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      ">0.2%",
      "not IE 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-interactions": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.7",
    "@storybook/manager-webpack5": "^6.5.7",
    "@storybook/node-logger": "^6.5.7",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.7",
    "@storybook/testing-library": "^0.0.11",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.5.4",
    "cross-env": "^7.0.3",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.5.12",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "node-static": "^0.7.11",
    "prettier": "^2.3.2",
    "react-refresh": "^0.11.0",
    "source-map-explorer": "^2.5.2",
    "webpack": "^5.73.0"
  },
  "resolutions": {
    "@svgr/webpack": "6.2.1",
    "react-refresh": "0.11.0"
  }
}
