  },
  "proxy": "https://incident-commander.canary.lab.flanksource.com",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development npx eslint --fix --ext .js,.jsx,.ts,.tsx"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "cross-env NODE_ENV=development npx prettier --write"
    ]
  },
  "scripts": {
    "dev": "rm -f middleware.ts && cross-env next dev",
    "dev:canary": "rm -f middleware.ts && cross-env NEXT_PUBLIC_APP_DEPLOYMENT=CANARY_CHECKER next dev",
    "dev:clerk": "cp clerk.middleware.ts middleware.ts && cross-env next dev",
    "start": "cross-env next start",
    "build": "node -v && rm -f middleware.ts && cross-env NODE_ENV=production next build",
    "build:clerk": "node -v && cp clerk.middleware.ts middleware.ts && cross-env NODE_ENV=production next build",
    "serve-build": "gzip -f --keep build/static/*/*.{js,css} && node scripts/serve-build.js",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "lint:eslint": "cross-env NODE_ENV=development eslint --ext .js,.jsx,.ts,.tsx .",
    "fix:eslint": "cross-env NODE_ENV=development eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check:browserslist": "npx browserslist",
    "prepare": "husky install",
    "launch:dev:canary-checker": "NODE_ENV=development APP_DEPLOYMENT=CANARY_CHECKER node scripts/serve-build.js",
    "launch:dev:incident-manager": "NODE_ENV=development APP_DEPLOYMENT=INCIDENT_MANAGER node scripts/serve-build.js",
    "svgo": "rm -rf src/icons/output; svgo -i src/icons/*.svg -o src/icons/output --config svgo.config.mjs",
    "svg-resize": "cd src/icons && bash resize.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      "defaults",
      ">0.2%",
      "not IE 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
